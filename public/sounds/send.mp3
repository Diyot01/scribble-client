  console.log("User connected:", socket.id);

  socket.on("joinRoom", ({ name, room }) =&gt; {
    socket.join(room);
    socket.name = name;
    socket.room = room;

    if (!rooms[room]) rooms[room] = [];

    rooms[room].push({ id: socket.id, name, score: 0 });

    io.to(room).emit("players", rooms[room]);

    if (!drawers[room]) {
      pickDrawer(room);
      startTimer(room);
    }

  });

  socket.on("draw", ({ room, x0, y0, x1, y1, color, size }) =&gt; {
    if (socket.id !== drawers[room]) return; // only drawer can draw
    socket.to(room).emit("draw", { x0, y0, x1, y1, color, size });
  });

  socket.on("guess", (text) =&gt; {
    const room = socket.room;
    if (!room) return;

    if (text.toLowerCase() === words[room]) {
      // Give points
      rooms[room].forEach(p =&gt; {
        if (p.id === socket.id) p.score += 100;       // guesser
        if (p.id === drawers[room]) p.score += 50;   // drawer
      });

      // Send updated leaderboard
      io.to(room).emit("players", rooms[room]);

      // Announce winner
      io.to(room).emit("message", {
        name: "SYSTEM",
        text: <code>${socket.name} guessed correctly!  (+100)</code>
      });

      // New round
      pickDrawer(room);
      io.to(room).emit("newRound");
      io.to(drawers[room]).emit("word", words[room]);
      startTimer(room);
    } else {
      io.to(room).emit("message", { name: socket.name, text });
    }
  });

  socket.on("disconnect", () =&gt; {
    const room = socket.room;
    if (!room) return;

    rooms[room] = rooms[room].filter(p =&gt; p.id !== socket.id);

    if (drawers[room] === socket.id) {
      pickDrawer(room);
    }

    io.to(room).emit("players", rooms[room]);
  });
});

server.listen(5000, () =&gt; {
