const express = require("express");
const http = require("http");
const { Server } = require("socket.io");
const cors = require("cors");

const app = express();
app.use(cors());

const server = http.createServer(app);
const io = new Server(server, {
  cors: {
    origin: "http://localhost:3000",
    methods: ["GET", "POST"]
  }
});

const rooms = {}; // room -&gt; [{id,name,score}]
const words = {};
const drawers = {};

const wordList = ["apple", "car", "dog", "phone", "tree", "house"];

function getRandomWord() {
  return wordList[Math.floor(Math.random() * wordList.length)];
}

function pickDrawer(room) {
  if (!rooms[room] || rooms[room].length === 0) return;

  const randomPlayer = rooms[room][Math.floor(Math.random() * rooms[room].length)];
  drawers[room] = randomPlayer.id;
  words[room] = getRandomWord();

  io.to(room).emit("drawer", drawers[room]);
  io.to(drawers[room]).emit("word", words[room]);
}

io.on("connection", (socket) =&gt; {
  console.log("User connected:", socket.id);

  socket.on("joinRoom", ({ name, room }) =&gt; {
    socket.join(room);
    socket.name = name;
    socket.room = room;

    if (!rooms[room]) rooms[room] = [];

    rooms[room].push({ id: socket.id, name, score: 0 });

    io.to(room).emit("players", rooms[room]);

    if (!drawers[room]) {
      pickDrawer(room);
    }
  });

  socket.on("draw", ({ room, x0, y0, x1, y1 }) =&gt; {
    if (socket.id !== drawers[room]) return;
    socket.to(room).emit("draw", { x0, y0, x1, y1 });
  });

  socket.on("guess", (text) =&gt; {
    const room = socket.room;
    if (!room) return;

    if (text.toLowerCase() === words[room]) {
      io.to(room).emit("message", {
        name: "SYSTEM",
        text: <code>${socket.name} guessed correctly! </code>
      });

      pickDrawer(room);
      io.to(room).emit("newRound");
      io.to(drawers[room]).emit("word", words[room]);
    } else {
      io.to(room).emit("message", { name: socket.name, text });
    }
  });

  socket.on("disconnect", () =&gt; {
    const room = socket.room;
    if (!room) return;

    rooms[room] = rooms[room].filter(p =&gt; p.id !== socket.id);

    if (drawers[room] === socket.id) {
      pickDrawer(room);
    }

    io.to(room).emit("players", rooms[room]);
  });
});

