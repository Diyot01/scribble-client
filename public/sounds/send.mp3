  timeLeft[room] = </span><span><span class="hljs-number">60</span></span><span>;
  io.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"time"</span></span><span>, timeLeft[room]);

