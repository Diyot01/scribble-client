</span><span><span class="hljs-keyword">const</span></span><span> { </span><span><span class="hljs-title class_">Server</span></span><span> } = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">"socket.io"</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> cors = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">"cors"</span></span><span>);

</span><span><span class="hljs-keyword">const</span></span><span> app = </span><span><span class="hljs-title function_">express</span></span><span>();
app.</span><span><span class="hljs-title function_">use</span></span><span>(</span><span><span class="hljs-title function_">cors</span></span><span>());

</span><span><span class="hljs-keyword">const</span></span><span> server = http.</span><span><span class="hljs-title function_">createServer</span></span><span>(app);
</span><span><span class="hljs-keyword">const</span></span><span> io = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Server</span></span><span>(server, {
