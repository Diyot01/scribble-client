  </span><span><span class="hljs-keyword">const</span></span><span> [drawing, setDrawing] = </span><span><span class="hljs-title function_">useState</span></span><span>(</span><span><span class="hljs-literal">false</span></span><span>);

  </span><span><span class="hljs-title function_">useEffect</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> canvas = canvasRef.</span><span><span class="hljs-property">current</span></span><span>;
