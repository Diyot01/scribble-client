<h1 data-start="238" data-end="278"> Step 1  Update Backend (server.js)</h1>
<p data-start="280" data-end="317">Replace your <code data-start="293" data-end="304">server.js</code> with this </p>
<pre class="overflow-visible! px-0!" data-start="319" data-end="2179"><div class="contain-inline-size rounded-2xl corner-superellipse/1.1 relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary select-none rounded-t-2xl corner-t-superellipse/1.1">js</div><div class="sticky top-[calc(--spacing(9)+var(--header-height))] @w-xl/main:top-9"><div class="absolute end-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-bg-elevated-secondary text-token-text-secondary flex items-center gap-4 rounded-sm px-2 font-sans text-xs"><button class="flex gap-1 items-center select-none py-1" aria-label="Copy"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" aria-hidden="true" class="icon-sm"><use href="/cdn/assets/sprites-core-k5zux585.svg#ce3544" fill="currentColor"></use></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-js"><span><span><span class="hljs-keyword">const</span></span><span> express = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">"express"</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> http = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">"http"</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> { </span><span><span class="hljs-title class_">Server</span></span><span> } = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">"socket.io"</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> cors = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">"cors"</span></span><span>);

</span><span><span class="hljs-keyword">const</span></span><span> app = </span><span><span class="hljs-title function_">express</span></span><span>();
app.</span><span><span class="hljs-title function_">use</span></span><span>(</span><span><span class="hljs-title function_">cors</span></span><span>());

</span><span><span class="hljs-keyword">const</span></span><span> server = http.</span><span><span class="hljs-title function_">createServer</span></span><span>(app);
</span><span><span class="hljs-keyword">const</span></span><span> io = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Server</span></span><span>(server, {
  </span><span><span class="hljs-attr">cors</span></span><span>: { </span><span><span class="hljs-attr">origin</span></span><span>: </span><span><span class="hljs-string">"*"</span></span><span> }
});

</span><span><span class="hljs-keyword">const</span></span><span> rooms = {}; </span><span><span class="hljs-comment">// room -&gt; players</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> words = {}; </span><span><span class="hljs-comment">// room -&gt; current word</span></span><span>

</span><span><span class="hljs-keyword">const</span></span><span> wordList = [</span><span><span class="hljs-string">"apple"</span></span><span>, </span><span><span class="hljs-string">"car"</span></span><span>, </span><span><span class="hljs-string">"dog"</span></span><span>, </span><span><span class="hljs-string">"phone"</span></span><span>, </span><span><span class="hljs-string">"tree"</span></span><span>, </span><span><span class="hljs-string">"house"</span></span><span>];

io.</span><span><span class="hljs-title function_">on</span></span><span>(</span><span><span class="hljs-string">"connection"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">socket</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">"User connected:"</span></span><span>, socket.</span><span><span class="hljs-property">id</span></span><span>);

  socket.</span><span><span class="hljs-title function_">on</span></span><span>(</span><span><span class="hljs-string">"joinRoom"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">{ name, room }</span></span></span><span>) =&gt; {
    socket.</span><span><span class="hljs-title function_">join</span></span><span>(room);
    socket.</span><span><span class="hljs-property">name</span></span><span> = name;
    socket.</span><span><span class="hljs-property">room</span></span><span> = room;

    </span><span><span class="hljs-keyword">if</span></span><span> (!rooms[room]) {
      rooms[room] = [];
      words[room] = wordList[</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">floor</span></span><span>(</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">random</span></span><span>() * wordList.</span><span><span class="hljs-property">length</span></span><span>)];
    }

    rooms[room].</span><span><span class="hljs-title function_">push</span></span><span>({ </span><span><span class="hljs-attr">id</span></span><span>: socket.</span><span><span class="hljs-property">id</span></span><span>, name });

    io.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"players"</span></span><span>, rooms[room]);
    socket.</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"word"</span></span><span>, words[room]); </span><span><span class="hljs-comment">// drawer will use this later</span></span><span>
  });

  socket.</span><span><span class="hljs-title function_">on</span></span><span>(</span><span><span class="hljs-string">"draw"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">{ room, x0, y0, x1, y1 }</span></span></span><span>) =&gt; {
    socket.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"draw"</span></span><span>, { x0, y0, x1, y1 });
  });

  socket.</span><span><span class="hljs-title function_">on</span></span><span>(</span><span><span class="hljs-string">"guess"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">text</span></span></span><span>) =&gt; {
    </span><span><span class="hljs-keyword">const</span></span><span> room = socket.</span><span><span class="hljs-property">room</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> name = socket.</span><span><span class="hljs-property">name</span></span><span>;

    </span><span><span class="hljs-keyword">if</span></span><span> (!room) </span><span><span class="hljs-keyword">return</span></span><span>;

    </span><span><span class="hljs-keyword">if</span></span><span> (text.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>() === words[room]) {
      io.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"message"</span></span><span>, {
        </span><span><span class="hljs-attr">name</span></span><span>: </span><span><span class="hljs-string">"SYSTEM"</span></span><span>,
        </span><span><span class="hljs-attr">text</span></span><span>: </span><span><span class="hljs-string">`<span class="hljs-subst">${name}</span></span></span><span> guessed correctly! `
      });

      </span><span><span class="hljs-comment">// new word</span></span><span>
      words[room] = wordList[</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">floor</span></span><span>(</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">random</span></span><span>() * wordList.</span><span><span class="hljs-property">length</span></span><span>)];
      io.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"newRound"</span></span><span>);
      socket.</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"word"</span></span><span>, words[room]);
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      io.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"message"</span></span><span>, { name, text });
    }
  });

  socket.</span><span><span class="hljs-title function_">on</span></span><span>(</span><span><span class="hljs-string">"disconnect"</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> room = socket.</span><span><span class="hljs-property">room</span></span><span>;
    </span><span><span class="hljs-keyword">if</span></span><span> (!room) </span><span><span class="hljs-keyword">return</span></span><span>;

    rooms[room] = rooms[room].</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">p</span></span></span><span> =&gt; p.</span><span><span class="hljs-property">id</span></span><span> !== socket.</span><span><span class="hljs-property">id</span></span><span>);
    io.</span><span><span class="hljs-title function_">to</span></span><span>(room).</span><span><span class="hljs-title function_">emit</span></span><span>(</span><span><span class="hljs-string">"players"</span></span><span>, rooms[room]);
  });
});

server.</span><span><span class="hljs-title function_">listen</span></span><span>(</span><span><span class="hljs-number">5000</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">"Server running on port 5000"</span></span><span>);
});
