    </span><span><span class="hljs-keyword">const</span></span><span> ctx = canvas.</span><span><span class="hljs-title function_">getContext</span></span><span>(</span><span><span class="hljs-string">"2d"</span></span><span>);
    ctx.</span><span><span class="hljs-property">lineCap</span></span><span> = </span><span><span class="hljs-string">"round"</span></span><span>;
    ctx.</span><span><span class="hljs-property">lineWidth</span></span><span> = </span><span><span class="hljs-number">4</span></span><span>;
