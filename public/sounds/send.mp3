    ctxRef.</span><span><span class="hljs-property">current</span></span><span> = ctx;

    socket.</span><span><span class="hljs-title function_">on</span></span><span>(</span><span><span class="hljs-string">"draw"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">{ x0, y0, x1, y1 }</span></span></span><span>) =&gt; {
