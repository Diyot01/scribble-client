    });

    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-function">() =&gt;</span></span><span> socket.</span><span><span class="hljs-title function_">off</span></span><span>(</span><span><span class="hljs-string">"draw"</span></span><span>);
  }, []);

  </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">startDrawing</span></span><span> = (</span><span><span class="hljs-params">e</span></span><span>) =&gt; {
    </span><span><span class="hljs-title function_">setDrawing</span></span><span>(</span><span><span class="hljs-literal">true</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> { offsetX, offsetY } = e.</span><span><span class="hljs-property">nativeEvent</span></span><span>;
