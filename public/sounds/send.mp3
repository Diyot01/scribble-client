    ctxRef.</span><span><span class="hljs-property">current</span></span><span>.</span><span><span class="hljs-property">lastY</span></span><span> = offsetY;
  };

  </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">stopDrawing</span></span><span> = (</span><span><span class="hljs-params"></span></span><span>) =&gt; {
    </span><span><span class="hljs-title function_">setDrawing</span></span><span>(</span><span><span class="hljs-literal">false</span></span><span>);
  };

  </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">draw</span></span><span> = (</span><span><span class="hljs-params">e</span></span><span>) =&gt; {
    </span><span><span class="hljs-keyword">if</span></span><span> (!drawing) </span><span><span class="hljs-keyword">return</span></span><span>;

    </span><span><span class="hljs-keyword">const</span></span><span> { offsetX, offsetY } = e.</span><span><span class="hljs-property">nativeEvent</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> { lastX, lastY } = ctxRef.</span><span><span class="hljs-property">current</span></span><span>;

    </span><span><span class="hljs-title function_">drawLine</span></span><span>(lastX, lastY, offsetX, offsetY, </span><span><span class="hljs-literal">true</span></span><span>);

    ctxRef.</span><span><span class="hljs-property">current</span></span><span>.</span><span><span class="hljs-property">lastX</span></span><span> = offsetX;
    ctxRef.</span><span><span class="hljs-property">current</span></span><span>.</span><span><span class="hljs-property">lastY</span></span><span> = offsetY;
