function pickDrawer(room) {
  if (!rooms[room] || rooms[room].length === 0) return;

  if (drawerIndex[room] === undefined) {
    drawerIndex[room] = 0;
  } else {
    drawerIndex[room] = (drawerIndex[room] + 1) % rooms[room].length;
  }

  const player = rooms[room][drawerIndex[room]];

  drawers[room] = player.id;
  words[room] = getRandomWord();

  io.to(room).emit("drawer", drawers[room]);
  io.to(drawers[room]).emit("word", words[room]);
}


function startTimer(room) {
  if (timers[room]) clearInterval(timers[room]);

  timeLeft[room] = 60;
  io.to(room).emit("time", timeLeft[room]);

  timers[room] = setInterval(() =&gt; {
    timeLeft[room]--;
    io.to(room).emit("time", timeLeft[room]);

    if (timeLeft[room] &lt;= 0) {
      clearInterval(timers[room]);

      io.to(room).emit("message", {
        name: "SYSTEM",
        text: " Time's up!"
      });

      pickDrawer(room);
      io.to(room).emit("newRound");
      io.to(drawers[room]).emit("word", words[room]);

      startTimer(room);
    }
  }, 1000);
}


